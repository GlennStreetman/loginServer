{
    "name": "login-service",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3001",
        "build": "next build",
        "start": "next start -p 3001",
        "dUp": "cp dev.env .env && docker-compose -f compose.dev.yaml up",
        "pBuild": "cp prod.env .env && sudo rm -rf .next && docker-compose -f compose.build.yaml up --build",
        "pUp": "cp prod.env .env && docker-compose -f compose.prod.yaml up",
        "lint": "next lint",
        "pushBuild": "docker ps | grep loginserver_loginserver | awk {'print $1'} | xargs -i docker commit {} login &&  docker tag login ghcr.io/glennstreetman/login:latest && docker push ghcr.io/glennstreetman/login:latest",
        "pull": "docker pull ghcr.io/glennstreetman/login:latest"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.17",
        "@next-auth/prisma-adapter": "^1.0.1",
        "@prisma/client": "^4.1.1",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tippyjs/react": "^4.2.6",
        "axios": "^0.26.0",
        "cookie": "^0.4.2",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-react-hooks": "^4.3.0",
        "next": "12.0.9",
        "next-auth": "^4.2.1",
        "next-connect": "^0.12.1",
        "nodemailer": "^6.7.2",
        "passport": "^0.5.2",
        "pg": "^8.7.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-icons": "^4.3.1"
    },
    "devDependencies": {
        "@types/node": "^17.0.14",
        "@types/react": "17.0.38",
        "autoprefixer": "^10.4.2",
        "eslint": "8.8.0",
        "eslint-config-next": "12.0.9",
        "postcss": "^8.4.5",
        "prisma": "^4.1.1",
        "tailwindcss": "^3.0.18",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    },
    "compilerOptions": {
        "sourceMap": true,
        "outDir": "dist",
        "strict": true,
        "lib": [
            "esnext"
        ],
        "esModuleInterop": true
    },
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    }
}
